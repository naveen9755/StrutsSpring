<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE hibernate-mapping PUBLIC "-//Hibernate/Hibernate Mapping DTD 3.0//EN" "http://hibernate.sourceforge.net/hibernate-mapping-3.0.dtd" >
<hibernate-mapping package="com.dto" catalog="assignmentaccount">
	<class name="UserDTO" table="USER">
		<id name="id" column="ID">
			<generator class="increment"></generator>
		</id>
		<property name="firstName" column="FIRSTNAME"></property>
		<property name="lastName" column="LASTNAME"></property>
		<property name="login" column="LOGIN"></property>
		<property name="password" column="PASSWORD"></property>
		<property name="dateOfBirth" column="DATEOFBIRTH"
			type="java.sql.Date">
		</property>
		<property name="lastAccessTime" column="LASTACCESSTIME"></property>
		<property name="securityQuestion" column="SECURITYQUESTION"></property>
		<property name="answer" column="ANSWER"></property>
		<property name="image" column="IMAGE"></property>
		<property name="mobileNumber" column="MOBILENUMBER"></property>
		<property name="lockSummery" column="LOCKSUMMERY"></property>
		<property name="role" column="ROLE"></property>
		<property name="count" column="COUNT"></property>
		<property name="gender" column="GENDER"></property>
	</class>

</hibernate-mapping>